{
  "name": "cypress-sidenis-demo",
  "version": "1.0.0",
  "scripts": {
    "start:client": "cd client && yarn start",
    "start:server": "cd server && yarn start",
    "serve:server": "cd server && yarn serve",
    "start": "concurrently --kill-others \"npm run serve:server\" \"npm run start:client\"",
    "cy": "cypress open"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cypress": "^3.3.0"
  }
}
